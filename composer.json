{
    "name": "ubermanu/migraine",
    "description": "Task runner oriented on data migration",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel Vodor",
            "email": "manu@paca.happy-dev.fr",
            "homepage": "https://github.com/ubermanu"
        }
    ],
    "require": {
        "php": "^8.0",
        "hoa/ruler": "~2.0",
        "symfony/yaml": "^5.1",
        "symfony/console": "^5.1",
        "doctrine/annotations": "^1.11"
    },
    "suggest": {
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "^9.4",
        "phpstan/phpstan": "^0.12.85"
    },
    "autoload": {
        "psr-4": {
            "Migraine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migraine\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/migraine"
    ]
}
